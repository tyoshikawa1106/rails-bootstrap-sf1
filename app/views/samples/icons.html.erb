<!-- Docs page layout -->
<div class="page-header page-header-showcase" id="content">
  <div class="container">
    <!-- Page Header -->
    <%= render 'samples/icons/page_header' %>
  </div>
</div>

<div class="container bs-docs-container">
  <div class="row">
    <div class="col-md-9">
      <section class="section" id="introduction">
        <!-- Introduction -->
        <%= render 'samples/icons/introduction' %>
      </section>
      <section class="section" id="sizes">
        <!-- Sizes -->
        <%= render 'samples/icons/sizes' %>
      </section>
      <section class="section" id="standard">
        <!-- Standard Icons -->
        <%= render 'samples/icons/standard' %>
      </section>
      <section class="section" id="friendly-custom">
        <!-- Friendlier Custom Icons -->
        <%= render 'samples/icons/friendly_custom' %>
      </section>
      <section class="section" id="custom">
        <!-- Custom Icons -->
        <%= render 'samples/icons/custom' %>
      </section>
      <section class="section" id="utility">
        <!-- Utility Icons -->
        <%= render 'samples/icons/utility' %>
      </section>
    </div>
    <div class="col-md-3">
      <!-- Nav -->
      <%= render 'samples/icons/nav' %>
    </div>
  </div>
</div>